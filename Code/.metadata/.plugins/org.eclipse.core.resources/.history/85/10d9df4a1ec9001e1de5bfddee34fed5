//in reference manual user push button is in PC13 pin 2
#include <stdint.h>
#include <stdio.h>
#include "stm32f4xx.h"






int __io_putchar(int ch)
{
	uart2_write(ch);
	return ch;
}

int main(void)
{


	uar2_tx_init();

	while(1)
	{
//		call the corrector
		printf("Hello from STM32F4.....\n\r");
	}
}


